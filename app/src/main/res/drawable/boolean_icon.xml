<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<!-- Using enabled to toggle the no-data drawable-->
	<item android:state_enabled="true" android:drawable="@drawable/checkmark_null"/>
	<item android:state_checked="true" android:drawable="@drawable/checkmark_on"/>
	<item android:state_checked="false" android:drawable="@drawable/checkmark_off"/>
</selector>